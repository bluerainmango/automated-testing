  "author": "Emily Yu",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "mocha-steps": "^1.3.0",
    "puppeteer": "^3.3.0",
    "puppeteer-firefox": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
{
  "name": "puppeteer-tricks",
  "version": "1.0.0",
  "description": "tips and tricks with puppeteer",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --timeout=30000 ./build/tests/**.js",
    "test:steps": "./node_modules/mocha/bin/mocha --timeout=30000 --require mocha-steps ./build/tests/**.js",
    "clean": "rm -rf build",
    "build": "babel --presets es2015 -d build/tests tests/"
